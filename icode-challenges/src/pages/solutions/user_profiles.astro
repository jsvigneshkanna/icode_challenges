---
import BaseLayout from "../../layout/baseLayout.astro";
---

<BaseLayout title="User Profiles">
  <div class="min-h-screen flex-col flex justify-center items-center py-32 shadow-lg text-black relative bg-white">
    <div class="w-full h-[60vh] md:h-[35vh] bg-gradient-to-tr from-purple-500 to-rose-300 absolute top-0 left-0 z-0">
    </div>
    <div class="w-full h-[2vh] bg-slate-200 top-[60vh] md:top-[35vh] left-0 z-0 absolute"></div>

    <!-- main panel -->
