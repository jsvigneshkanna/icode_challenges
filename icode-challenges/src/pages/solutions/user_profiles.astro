        class="rounded-md shadow-lg min-w-[260px] shadow-slate-700/50 hover:shadow-orange-800/40 pt-10 bg-white flex flex-col justify-center items-center relative"
      >
        <!-- setting icons -->
        <div class="text-gray-500 absolute top-4 right-4 hover:text-black hover:scale-110 cursor-pointer">
          <svg
            xmlns="http://www.w3.org/2000/svg"
            fill="none"
            viewBox="0 0 24 24"
            stroke-width="1.3"
            stroke="currentColor"
            class="w-6 h-6"
          >
            <path
              stroke-linecap="round"
              stroke-linejoin="round"
              d="M6.75 12a.75.75 0 11-1.5 0 .75.75 0 011.5 0zM12.75 12a.75.75 0 11-1.5 0 .75.75 0 011.5 0zM18.75 12a.75.75 0 11-1.5 0 .75.75 0 011.5 0z"
            ></path>
